                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic
' The ScriptMain class is the entry point of the Script Task.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
Imports ADODB

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
    Dim adoConn As New ADODB.Connection
    Dim adoRS As New ADODB.Recordset
    Dim adoCMD As New ADODB.Command

    Sub WriteToWorkImportLog(ByVal StatusCode As Integer, ByVal Comments As String)
        With adoCMD
            .ActiveConnection = adoConn
            .CommandType = ADODB.CommandTypeEnum.adCmdStoredProc
            .CommandText = "WorkImportLog_INSERT"
            .Parameters("@AttachmentForceID").Value = adoRS.Fields("AttachmentForceID").Value
            .Parameters("@StatusCode").Value = StatusCode
            If Len(Comments) > 0 Then
                .Parameters("@StatusNotes").Value = Comments
            End If
            .Execute()
        End With
    End Sub

    Sub SaveBinaryData(ByVal Filename As String, ByVal Bytearray As Object)
        On Error Resume Next

        Dim Strfullfilename As String

        'create Stream Object
        Dim Binarystream As New ADODB.Stream

        'specify Stream Type - We Want To Save Binary Data.
        Binarystream.Type = ADODB.StreamTypeEnum.adTypeBinary

        'open The Stream And Write Binary Data To The Object
        Binarystream.Open()
        Binarystream.Write(Bytearray)

        'save Binary Data To Disk

        Binarystream.SaveToFile(Filename, ADODB.SaveOptionsEnum.adSaveCreateOverWrite)
        If Err.Number <> 0 Then
            Call WriteToWorkImportLog(-1, "Failed to save file to disk " & Err.Description & " : " & Filename)
        Else
            Call WriteToWorkImportLog(1200, "File created" & " : " & Filename)
        End If

    End Sub


    ' The execution engine calls this method when the task executes.
    ' To access the object model, use the Dts object. Connections, variables, events,
    ' and logging features are available as static members of the Dts class.
    ' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
    ' 
    ' To open Code and Text Editor Help, press F1.
    ' To open Object Browser, press Ctrl+Alt+J.

    Public Sub Main()

        Dim strServer As String
        Dim strDatabase As String
        Dim strSQL As String
        Dim strImportObject As String
        Dim strImportType As String

        strServer = CStr(Dts.Variables("$Package::Server").Value)
        strDatabase = CStr(Dts.Variables("$Package::Database").Value)
        strImportType = CStr(Dts.Variables("$Package::ImportType").Value)
        strImportObject = CStr(Dts.Variables("$Package::ImportObject").Value)

        strSQL = "SELECT dbo.ATTACHMENT.NAMEX AS AttachmentName, dbo.ATTACHMENT.BODYX, dbo.ATTACHMENT.ID AS AttachmentForceID, 	dbo.WorkImportHeader.StatusCode, " & _
                "ForceAttachmentPaths.DownloadPath + CAST(ATTACHMENT.ETL_ID AS varchar(10)) + ATTACHMENT.NAMEX AS FullExportFileName " & _
                "FROM dbo.ATTACHMENT " & _
                "INNER JOIN " + strImportObject + " ON dbo.ATTACHMENT.PARENTID = " + strImportObject + ".ID " & _
                "INNER JOIN ForceAttachmentPaths ON " + strImportObject + ".ID = dbo.ForceAttachmentPaths.ForceImportID " & _
                "INNER JOIN WorkImportHeader ON dbo.ATTACHMENT.ID = dbo.WorkImportHeader.AttachmentForceID " & _
                "WHERE(ForceAttachmentPaths.Description = '" + strImportType + "' And WorkImportHeader.StatusCode = 1000)"


        With adoConn
            .ConnectionString = "Data Source=" & strServer & ";Initial Catalog=" & strDatabase & ";Provider=SQLNCLI.1;Integrated Security=SSPI;Auto Translate=False;"
            .Open()
        End With

        With adoRS
            .Open(strSQL, adoConn)
            Do While Not .EOF
                Call SaveBinaryData(CStr(.Fields("FullExportFileName").Value), .Fields("bodyX").Value)
                .MoveNext()
            Loop
            .Close()
        End With

    End Sub


End Class]]></ProjectItem>
