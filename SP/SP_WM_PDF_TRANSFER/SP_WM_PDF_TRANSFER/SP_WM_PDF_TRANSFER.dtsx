        DTS:DataType="8">SELECT 'E:\ImportRoutineFiles\SP_WM_PDF\' +RIGHT(WORK_INSTRUCTION.NAMEX,6) + '\'+ ATTACHMENT.NAMEX AS FULLPATHFILENAME, ATTACHMENT.BODYX
FROM  HoldingTable_WI    JOIN  MOBILE_WORKING ON HoldingTable_WI.ID = MOBILE_WORKING.ID

JOIN  WORK_INSTRUCTION ON MOBILE_WORKING.WMS_REFERENCE = WORK_INSTRUCTION.ID

JOIN  ATTACHMENT ON MOBILE_WORKING.ID = ATTACHMENT.PARENTID

WHERE HoldingTable_WI.StatusCode = 20 AND MOBILE_WORKING.ID = ''</DTS:VariableValue>
