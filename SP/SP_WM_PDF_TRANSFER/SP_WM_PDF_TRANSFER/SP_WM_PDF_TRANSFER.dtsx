      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT '&quot; + @[$Package::FilePathStub]   +&quot;' +RIGHT(WORK_INSTRUCTION.NAMEX,6) + '\\'+&quot; + &quot; ATTACHMENT.NAMEX AS FULLPATHFILENAME, ATTACHMENT.BODYX&#xA;FROM  HoldingTable_WI    JOIN  MOBILE_WORKING ON HoldingTable_WI.ID = MOBILE_WORKING.ID&#xA;&#xA;JOIN  WORK_INSTRUCTION ON MOBILE_WORKING.WMS_REFERENCE = WORK_INSTRUCTION.ID&#xA;&#xA;JOIN  ATTACHMENT ON MOBILE_WORKING.ID = ATTACHMENT.PARENTID&#xA;&#xA;WHERE HoldingTable_WI.StatusCode = 20 AND MOBILE_WORKING.ID = '&quot; +  @[User::WM_ID] + &quot;'&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQLToExecute">
